<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.shenyu.laikaword.module.shop.activity.PickUpSuccessActivity">
<include layout="@layout/toolbar_layout"/>
    <TextView
        android:layout_marginTop="76dp"
        android:drawableTop="@mipmap/state_ok"
        android:layout_gravity="center_horizontal"
        android:text="申请完成"
        android:drawablePadding="18dp"
        android:textSize="@dimen/text_size_18sp"
        android:textColor="@color/color_333"
        android:gravity="center_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <LinearLayout
        android:layout_marginTop="154dp"
        android:layout_gravity="center_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/tv_sq_check_state"
            style="@style/tv_operation_finsh"
            android:text="查看状态"
             />
        <TextView

            android:layout_marginLeft="29dp"
            android:id="@+id/tv_sq_back_main"
            style="@style/tv_operation_finsh"
                android:text="返回首页"
            />
    </LinearLayout>

</LinearLayout>
