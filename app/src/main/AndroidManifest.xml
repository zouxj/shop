<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shenyu.laikaword">

    <uses-permission android:name="android.permission.MOUNT_FORMAT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <permission
        android:name="com.shenyu.laikaword.permission.JPUSH_MESSAGE"
        android:protectionLevel="signature" />

    <!-- Required -->
    <uses-permission android:name="com.shenyu.laikaword.permission.JPUSH_MESSAGE" />

    <application
        android:name=".LaiKaApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".main.activity.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name="android.support.v4.content.FileProvider"
            android:authorities="com.shenyu.laikaword.fileprovider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_provider_paths" />
        </provider>

        <service android:name=".http.downloadmanager.DownLoadService" />

        <activity android:name=".module.login.activity.LoginActivity" />
        <activity
            android:name=".main.activity.MainActivity"
            android:theme="@style/Main_AppTheme"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".main.activity.WelcomePageActivity" />

        <receiver
            android:name=".jpushreceiver.TalkReceiver"
            android:enabled="true">
            <intent-filter>

                <!-- Required 用户注册SDK的intent -->
                <action android:name="cn.jpush.android.intent.REGISTRATION" />
                <!-- Required 用户接收SDK消息的intent -->
                <action android:name="cn.jpush.android.intent.MESSAGE_RECEIVED" />
                <!-- Required 用户接收SDK通知栏信息的intent -->
                <action android:name="cn.jpush.android.intent.NOTIFICATION_RECEIVED" />
                <!-- Required 用户打开自定义通知栏的intent -->
                <action android:name="cn.jpush.android.intent.NOTIFICATION_OPENED" />
                <!-- 接收网络变化 连接/断开 since 1.6.3 -->
                <action android:name="cn.jpush.android.intent.CONNECTION" />

                <category android:name="com.shenyu.laikaword" />
            </intent-filter>
        </receiver>
        <receiver
            android:name=".jpushreceiver.AppJPushMessageReceiver"
            android:enabled="true"
            android:exported="true" />
        <!-- hotfix配置 -->
        <meta-data
            android:name="com.taobao.android.hotfix.IDSECRET"
            android:value="24659306-1" />
        <meta-data
            android:name="com.taobao.android.hotfix.APPSECRET"
            android:value="ecdc301c7fac7e942eeb76031d237e7b" />
        <meta-data
            android:name="com.taobao.android.hotfix.RSASECRET"
            android:value="" />

        <activity android:name=".module.mine.remaining.UserRemainingActivity" />
        <activity android:name=".module.mine.remaining.WthdrawMoneyActivity" />
        <activity android:name=".module.mine.remaining.RechargeMoneyActivity" />
        <activity android:name=".module.mine.remaining.PurchaseCardActivity" />
        <activity android:name=".module.mine.remaining.DetailMoneyActivity" />
        <activity android:name=".module.shop.activity.BuyGoodsActivity" />
        <activity android:name=".module.mine.cards.activity.CardPackageActivity" />
        <activity android:name=".module.mine.cards.activity.CardBankActivity" />
        <activity android:name=".module.mine.systemsetting.activity.SettingSystemActivity" />
        <activity android:name=".module.mine.systemsetting.activity.AboutAppActivity" />
        <activity android:name=".module.mine.systemsetting.activity.AccountBindActivity" />
        <activity android:name=".module.mine.systemsetting.activity.BoundPhoneActivity" />
        <activity android:name=".module.mine.systemsetting.activity.UserInfoActivity" />
        <activity
            android:name="com.leo618.mpermission.AppSettingsDialogHolderActivity"
            android:exported="false"
            android:theme="@style/MPermissions.Transparent" />
        <activity android:name=".module.mine.systemsetting.activity.UpdateUserNameActivity" />
        <activity android:name=".module.mine.cards.activity.AddBankCardActivity" />
        <activity
            android:name="com.tencent.tauth.AuthActivity"
            android:launchMode="singleTask"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="tencent101425071" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tencent.connect.common.AssistActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity
            android:name=".wxapi.WXEntryActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:launchMode="singleTop" />
        <activity android:name=".module.shop.activity.ConfirmOrderActivity" />
        <activity android:name=".module.shop.activity.PaySuccessActivity" />
        <activity android:name=".module.mine.cards.activity.CardBankInfoActivity" />
        <activity android:name=".module.mine.address.activity.AddressInfoActivity" />
        <activity android:name=".module.mine.address.activity.EditAddressActivity" />
        <activity android:name=".module.shop.activity.PickUpActivity" />
        <activity android:name=".module.shop.activity.ShopDateilActivity" />
        <activity android:name=".module.shop.activity.PickUpSuccessActivity" />
        <activity android:name=".module.mine.address.activity.SelectAddressActivity" />
        <activity android:name=".module.shop.activity.PickUpTelActivity" />
        <activity android:name=".module.shop.activity.PickUpStateActivity" />
        <activity android:name=".module.mine.setpassword.SetPassWordOneActivity" />
        <activity android:name=".module.mine.setpassword.SetPassWordTwoActivity" />
        <activity android:name=".module.mine.systemsetting.activity.AcountBdingSuccessActivity" />
        <activity android:name=".module.mine.setpassword.SetPassWordMsgCodeActivity" />
        <activity android:name=".module.mine.message.UserMessageActivity" />

        <provider
            android:name="android.support.v4.content.FileProvider"
            android:authorities="com.shenyu.laikaword.fileprovider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_provider_paths" />
        </provider>

        <activity android:name=".web.GuessActivity"></activity>
    </application>

</manifest>